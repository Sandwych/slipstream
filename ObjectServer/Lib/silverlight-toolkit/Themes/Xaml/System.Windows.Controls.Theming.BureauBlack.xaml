                        <ContentPresenter Margin="0,0,1,0" Grid.Column="1" Content="{TemplateBinding Content}" HorizontalAlignment="Left" VerticalAlignment="Center"/>
                        <Path x:Name="CurrentRowGlyph" Margin="8,0,8,0" Grid.RowSpan="2" Height="10" Width="6" Fill="{TemplateBinding SeparatorBrush}" Stretch="Fill" VerticalAlignment="Center" HorizontalAlignment="Center" Opacity="0" Data="F1 M 511.047,352.682L 511.047,342.252L 517.145,347.467L 511.047,352.682 Z "/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
