	                    				Margin="{TemplateBinding Padding}"  
	                    				HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
										VerticalAlignment="Center">
                                    <TextBlock Text=" "/>
                                </ContentPresenter>
                                <Rectangle x:Name="DisabledVisualElement" Fill="#A5FFFFFF" RadiusX="0" RadiusY="0" IsHitTestVisible="false" Visibility="Collapsed" />
                                <Rectangle x:Name="FocusVisualElement" Margin="-1" Stroke="#FFF28926" StrokeThickness="1" IsHitTestVisible="false" Visibility="Collapsed" />
