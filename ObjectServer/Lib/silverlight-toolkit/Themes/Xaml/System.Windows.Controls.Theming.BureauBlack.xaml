                                                            <GradientStop Color="#FFFCBF78" Offset="0.442"/>
                                                            <GradientStop Color="#FFFD8D2C" Offset="0.452"/>
                                                            <GradientStop Color="#FFFCA84B" Offset="0.802"/>
