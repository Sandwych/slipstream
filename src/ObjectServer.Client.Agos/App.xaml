<Application   
  x:Class="ObjectServer.Client.Agos.App"
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:app="clr-namespace:ObjectServer.Client.Agos"
  Startup="Application_Startup"
  UnhandledException="Application_UnhandledException">

    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Assets/Styles.xaml" />
                <ResourceDictionary>
                    <app:ResourceWrapper x:Key="ResourceWrapper" />
                    <app:NotOperatorValueConverter x:Key="NotOperatorValueConverter" />
                </ResourceDictionary>
                <ResourceDictionary Source="Assets/Themes/Windows7/Styles.xaml"/>
                <ResourceDictionary Source="Assets/Themes/Windows7/CoreStyles.xaml"/>
                <ResourceDictionary Source="Assets/Themes/Windows7/ToolkitStyles.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>

    </Application.Resources>

</Application>